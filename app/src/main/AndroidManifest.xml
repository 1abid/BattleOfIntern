<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="abid.challenge.ritetag.com.ritetagchallenge">

  <uses-permission android:name="android.permission.INTERNET" />

  <application
      android:allowBackup="false"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".login.TwitterLogInActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
        android:name=".trendingHashTag.TrendingHashTagActivity"
        android:theme="@style/AppTheme.OverlapSystemBar" />
    <activity android:name=".hashTagInfluence.HashTagInfluenceActivity"></activity>
  </application>

</manifest>